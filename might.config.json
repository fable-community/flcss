{
  "startCommand": "yarn vite dev",
  "url": "http://localhost:3000",
  "targets": [ "chromium", "firefox", "webkit" ],
  "viewport": {
    "width": null,
    "height": null
  },
  "titleBasedScreenshots": true,
  "parallelTests": 6,
  "defaultTimeout": 25000,
  "tolerance": 2.5,
  "antialiasingTolerance": 3.5,
  "pageErrorIgnore": [
    "net::ERR_ABORTED",
    "NS_BINDING_ABORTED",
    "access control checks",
    "Load request cancelled"
  ],
  "coverageExclude": [
    "/node_modules/**"
  ]
}
